<Window x:Class="GameDetail.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameDetail"
        mc:Ignorable="d"
        Title="MainWindow" Height="1080" Width="1920">
    <Grid>
        <Button x:Name="Btn_Read" Content="讀取成績" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="30" Width="100" Click="Btn_Read_Click" FontFamily="Arial Black" FontWeight="Bold" FontSize="14"/>
        <Button x:Name="Btn_Cookie" Content="設定Cookie" HorizontalAlignment="Left" Margin="120,10,0,0" VerticalAlignment="Top" Height="30" Width="100" Click="Btn_Cookie_Click" FontSize="14" FontFamily="Arial Black" FontWeight="Bold"/>
        <Button x:Name="Btn_Cls" Content="clean" HorizontalAlignment="Left" Margin="690,10,0,0" VerticalAlignment="Top" Height="30" Width="100" Click="Btn_Clean_Click"/>

        <ListBox x:Name="Listbox_Log" Margin="0,48,0,0" />
        <TextBox x:Name="Txt_CookieName" HorizontalAlignment="Left" Margin="225,10,0,0" TextWrapping="Wrap" Text="ASPSESSIONIDSQQQSCCA" VerticalAlignment="Top" Width="180" Height="30" VerticalContentAlignment="Center" FontSize="12" FontFamily="Microsoft YaHei UI" FontWeight="Bold"/>
        <TextBox x:Name="Txt_CookieValue" HorizontalAlignment="Left" Margin="410,10,0,0" TextWrapping="Wrap" Text="GDPPEADCBJHANCALEKHCENMP" VerticalAlignment="Top" Width="265" Height="30" VerticalContentAlignment="Center" FontSize="12" FontFamily="Microsoft YaHei UI" FontWeight="Bold"/>
    </Grid>
</Window>
